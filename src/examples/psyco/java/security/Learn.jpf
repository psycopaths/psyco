############################################################################### 
#
# experiment setup
#

psyco.interface.m1=psyco.java.security.Signature.initSign()
psyco.interface.m2=psyco.java.security.Signature.initVerify()
psyco.interface.m3=psyco.java.security.Signature.sign()
psyco.interface.m4=psyco.java.security.Signature.verify()
psyco.interface.m5=psyco.java.security.Signature.update()

psyco.depth=4

# outputs results - use tmp because I ignore it during commits 
# interface.outputFile=tmp/exampleProtocolSteffen2011


############################################################################### 
#
# default psyco tools
#

# psyco
shell=gov.nasa.jpf.psyco.Psyco

# jdart

# jpf-constraints
symbolic.dp=z3
symbolic.dp.z3.library.path=/home/falk/Projekte/jpf/jpf-constraints/lib/libZ3Java.so


############################################################################### 
#
# logging and jpf output
#
log.fine=psyco
log.info=jdart,constraints

# following line used to track what JPF executes
#listener=.listener.ExecTracker

# avoid statistics during learning process by enabling the following two lines
report.console.start=
report.console.finished=
report.console.property_violation=
